{
	"name": "@ton-ai-core/vibecode-linter",
	"version": "1.0.5",
	"type": "module",
	"description": "Advanced TypeScript linter with Git integration, dependency analysis, and comprehensive error reporting",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"bin": {
		"vibecode-linter": "dist/bin/vibecode-linter.js"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ton-ai-core/vibecode-linter.git"
	},
	"bugs": {
		"url": "https://github.com/ton-ai-core/vibecode-linter/issues"
	},
	"homepage": "https://github.com/ton-ai-core/vibecode-linter#readme",
	"scripts": {
		"build": "npx -y rimraf ./dist && tsc -p tsconfig.src.json",
		"prepack": "npm run build",
		"prepublishOnly": "npm run build",
		"release": "npm run build && npm run lint && npm version patch && npm publish --access public",
		"lint": "npx tsx src/bin/vibecode-linter.ts src/",
		"test": "npm run lint && npm run build && npx tsx src/bin/vibecode-linter.ts test/ && vitest run",
		"test:coverage": "vitest run --coverage",
		"mutation": "npm run lint && npx stryker run"
	},
	"keywords": [
		"linter",
		"typescript",
		"eslint",
		"biome",
		"git-integration",
		"code-quality",
		"static-analysis",
		"dependency-analysis",
		"commit-history"
	],
	"author": "ton-ai-core",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"overrides": {
		"picomatch": "^4.0.3"
	},
	"dependencies": {
		"ajv": "^8.17.1",
		"effect": "^3.19.0",
		"jiti": "^2.6.1",
		"jscpd": "^4.0.5",
		"jscpd-sarif-reporter": "^4.0.3",
		"loop-controls": "^1.1.0",
		"ts-pattern": "^5.9.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.3",
		"@effect/vitest": "^0.27.0",
		"@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
		"@eslint/js": "^9.39.1",
		"@stryker-mutator/core": "^9.3.0",
		"@stryker-mutator/typescript-checker": "^9.3.0",
		"@stryker-mutator/vitest-runner": "^9.3.0",
		"@ton-ai-core/eslint-plugin-suggest-members": "^1.6.17",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^24.10.0",
		"@types/node-fetch": "^2.6.13",
		"@typescript-eslint/eslint-plugin": "^8.46.3",
		"@typescript-eslint/parser": "^8.46.3",
		"@vitest/ui": "^4.0.7",
		"eslint": "^9.39.1",
		"eslint-import-resolver-node": "^0.3.9",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-fp-ts-strict": "^0.1.1",
		"eslint-plugin-functional": "^9.0.2",
		"eslint-plugin-import-x": "^4.16.1",
		"eslint-plugin-jsdoc": "^61.1.12",
		"eslint-plugin-jsonc": "^2.21.0",
		"eslint-plugin-n": "^17.23.1",
		"eslint-plugin-perfectionist": "^4.15.1",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-regexp": "^2.10.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-sonarjs": "^3.0.5",
		"eslint-plugin-sql": "^3.2.2",
		"eslint-plugin-sql-template": "^3.1.0",
		"eslint-plugin-sqlite": "^1.6.0",
		"eslint-plugin-typeorm-typescript": "^0.5.1",
		"eslint-plugin-unicorn": "^62.0.0",
		"eslint-plugin-unused-imports": "^4.3.0",
		"eslint-plugin-vitest": "^0.5.4",
		"eslint-plugin-yml": "^1.19.0",
		"eslint-rule-docs": "^1.1.235",
		"fast-check": "^4.3.0",
		"globals": "^16.5.0",
		"ts-add-js-extension": "^1.6.6",
		"ts-morph": "^27.0.2",
		"tsc-esm-fix": "^3.1.2",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.7"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
